{
  "name": "hello",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-easy-state": "6.1.3",
    "react-hotkeys": "1.1.4",
    "react-outside-click-handler": "1.2.3",
    "react-scripts": "3.0.1",
    "react-test-renderer": "16.8.6",
    "resize-observer-polyfill": "1.5.1",
    "styled-components": "4.3.2",
    "styled-normalize": "8.0.6",
    "typescript": "3.4.5",
    "wicg-inert": "2.1.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.134",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-outside-click-handler": "1.2.0",
    "@types/react-test-renderer": "16.8.2",
    "@types/styled-components": "4.1.8",
    "jest-styled-components": "6.3.3",
    "prettier": "1.18.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run verify && INLINE_RUNTIME_CHUNK=false react-scripts build",
    "test": "react-scripts test",
    "types": "tsc --pretty --noEmit",
    "prettier-check": "prettier --check '{src,public}/**/*.{ts,js,tsx,jsx,json,html}'",
    "prettier-write": "prettier --write '{src,public}/**/*.{ts,js,tsx,jsx,json,html}'",
    "verify": "npm run types && npm run prettier-check && CI=true npm run test",
    "zip": "cd build && zip -r -X \"../build.zip\" *"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 4 chrome versions",
    "last 4 firefox versions"
  ]
}
