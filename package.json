{
  "name": "hello",
  "version": "1.0.1",
  "description": "New tab page with daily image and bunch of useful things that you can display, like clock and stuff.",
  "license": "MIT",
  "author": {
    "name": "Petr Huřťák",
    "email": "petr.hurtak@gmail.com"
  },
  "dependencies": {
    "focus-visible": "5.0.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-easy-state": "6.1.3",
    "react-hotkeys": "2.0.0",
    "react-hotkeys-hook": "1.5.1",
    "react-scripts": "3.0.1",
    "resize-observer-polyfill": "1.5.1",
    "styled-components": "4.3.2",
    "styled-normalize": "8.0.6",
    "typescript": "3.4.5",
    "wicg-inert": "2.1.2"
  },
  "devDependencies": {
    "@types/css-font-loading-module": "0.0.2",
    "@types/jest": "24.0.17",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.8.5",
    "@types/react-test-renderer": "16.9.0",
    "@types/styled-components": "4.1.8",
    "jest-styled-components": "6.3.3",
    "np": "5.0.3",
    "prettier": "1.18.2",
    "react-test-renderer": "16.9.0",
    "source-map-explorer": "2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false REACT_APP_BUILD_TIME=$(date +%s) react-scripts build",
    "tests": "react-scripts test",
    "types": "tsc --pretty --noEmit",
    "analyze": "source-map-explorer ./build/static/js/*",
    "prettier-check": "prettier --check '{src,public}/**/*.{ts,js,tsx,jsx,json,html}'",
    "prettier-write": "prettier --write '{src,public}/**/*.{ts,js,tsx,jsx,json,html}'",
    "validate": "npm run types && npm run prettier-check && npm run tests -- --watchAll=false",
    "zip": "cd build && zip -r -X \"../build.zip\" *",
    "publish": "npm run validate && np --no-publish && npm run build && npm run zip"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      "last 4 chrome versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
